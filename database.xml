<?xml version="1.0" encoding="UTF-8"?>
<database>

    <global>
        <table name="watcher" no-site-reference="1" no-auto-update="1">
            <field type="INT(11) NOT NULL">id</field>
            <field type="TEXT NULL DEFAULT NULL">message</field>
            <field type="VARCHAR(255) NULL DEFAULT NULL">localeGroup</field>
            <field type="VARCHAR(255) NULL DEFAULT NULL">localeVar</field>
            <field type="TEXT NULL DEFAULT NULL">localeParams</field>
            <field type="INT(11) NOT NULL">uid</field>
            <field type="DATETIME NULL DEFAULT NULL">statusTime</field>
            <field type="VARCHAR(255) NULL DEFAULT NULL">call</field>
            <field type="TEXT NULL DEFAULT NULL">callParams</field>

            <primary>id</primary>
            <auto_increment>id</auto_increment>
        </table>

        <table name="watcherEvents" no-site-reference="1" no-auto-update="1">
            <field type="INT(11) NOT NULL">id</field>
            <field type="VARCHAR(255) NULL DEFAULT NULL">package</field>
            <field type="VARCHAR(255) NULL DEFAULT NULL">ajax</field>
            <field type="VARCHAR(255) NULL DEFAULT NULL">event</field>
            <field type="TEXT NULL DEFAULT NULL">exec</field>

            <primary>id</primary>
            <auto_increment>id</auto_increment>
        </table>
    </global>

</database>
